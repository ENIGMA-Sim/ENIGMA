<?xml version="1.0"?>
<!DOCTYPE platform
  SYSTEM 'http://simgrid.gforge.inria.fr/simgrid/simgrid.dtd'>

<platform version="4.1">
	<config>
		<prop id="debug/verbose-exit" value="0"/>
		<prop id="debug/clean-atexit" value="0"/>
	</config>
	<AS id="AS0" routing="Full">
		<cluster bw="300Mbps" core="4" id="IoT0" lat="50ms" prefix="iot-0-" radical="0-999" router_id="IoT_cluster0" speed="1.5Gf,1Gf,750Mf" suffix="">
			<prop id="wattage_per_state" value="0.7:0.8:0.9, 0.6:0.7:0.8, 0.5:0.6:0.7"/>
			<prop id="wattage_off" value="10"/>
			<prop id="ram" value="4GB"/>
		</cluster>
		<cluster bw="100Mbps" core="8" id="Dispatcher0" lat="10ms" prefix="d-0-" radical="0-2" router_id="Dispatcher_cluster0" speed="2Gf,1Gf,750Mf" suffix="">
			<prop id="wattage_per_state" value="100.0:120.0:140.0, 80.0:100.0:120.0, 60.0:80.0:100.0"/>
			<prop id="wattage_off" value="20"/>
			<prop id="ram" value="4GB"/>
		</cluster>
		<cluster bw="500Mbps" core="64" id="Datacenter0" lat="5ms" prefix="s-0-" radical="0-999" router_id="Datacenter_cluster0" speed="2Gf,1Gf,750Mf" suffix="">
			<prop id="wattage_per_state" value="100.0:120.0:140.0, 80.0:100.0:120.0, 60.0:80.0:100.0"/>
			<prop id="wattage_off" value="40"/>
			<prop id="ram" value="144000MB"/>
		</cluster>
		<cluster bw="125GBps" id="Controller" lat="0us" prefix="cont-" radical="0-1" router_id="Controller_cluster" speed="4Gf,2Gf,1Gf" suffix=""/>
		<link bandwidth="770Mbps" id="linkIoT0Dispatcher0" latency="770us"/>
		<link bandwidth="770Mbps" id="linkDispatcher0Datacenter0" latency="770us"/>
		<link bandwidth="770Mbps" id="linkDatacenter0IoT0" latency="770us"/>
		<link bandwidth="10GBps" id="linkControllerDispatcher0" latency="50us"/>
		<link bandwidth="10GBps" id="linkIoT0Controller" latency="50us"/>
		<ASroute dst="Dispatcher0" gw_dst="Dispatcher_cluster0" gw_src="IoT_cluster0" src="IoT0">
			<link_ctn id="linkIoT0Dispatcher0"/>
		</ASroute>
		<ASroute dst="Datacenter0" gw_dst="Datacenter_cluster0" gw_src="Dispatcher_cluster0" src="Dispatcher0">
			<link_ctn id="linkDispatcher0Datacenter0"/>
		</ASroute>
		<ASroute dst="IoT0" gw_dst="IoT_cluster0" gw_src="Datacenter_cluster0" src="Datacenter0">
			<link_ctn id="linkDatacenter0IoT0"/>
		</ASroute>
		<ASroute dst="Controller" gw_dst="Controller_cluster" gw_src="IoT_cluster0" src="IoT0">
			<link_ctn id="linkIoT0Controller"/>
		</ASroute>
		<ASroute dst="Dispatcher0" gw_dst="Dispatcher_cluster0" gw_src="Controller_cluster" src="Controller">
			<link_ctn id="linkControllerDispatcher0"/>
		</ASroute>
	</AS>
</platform>
